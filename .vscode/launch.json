{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Excel Desktop (Edge Chromium)",
      "type": "chrome",
      "request": "attach",
      "url": "https://excel.architect.co/taskpane.html",
      "webRoot": "${workspaceRoot}/docs",
      "preLaunchTask": "Debug: Excel Desktop",
      "postDebugTask": "Stop Debug"
    },
    {
      "name": "Excel Desktop (Edge Legacy)",
      "type": "msedge",
      "request": "attach",
      "url": "https://excel.architect.co/taskpane.html",
      "timeout": 600000,
      "webRoot": "${workspaceRoot}/docs",
      "preLaunchTask": "Debug: Excel Desktop",
      "postDebugTask": "Stop Debug"
    }
  ]
}